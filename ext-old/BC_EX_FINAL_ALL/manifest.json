{
    "manifest_version": 2,
    "name": "ClassicTrenball",
    "version": "1.0",
    "description": "",
    "permissions": [
        "tabs",
        "declarativeContent",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": []
    },
    "browser_action": {
    },
    "content_scripts": [
        {
            "matches": [
                "https://bcgame.ai/game/crash",
                "https://bcgame.ai/game/crash?type=trenball"
            ],
            "js": [
                "crash.js"
            ]
        }
    ]
}