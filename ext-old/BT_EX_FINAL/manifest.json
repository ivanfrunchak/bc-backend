{
    "manifest_version": 2,
    "name": "BustaBit",
    "version": "1.0",
    "description": "",
    "permissions": [
        "tabs",
        "declarativeContent",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "debugger",
        "https://*/",
        "wss://*/"
    ],
    "background": {
        "scripts": ["bgp.js"],
        "persistent": true
    },
    "browser_action": {
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.bustabit.com/play"
            ],
            "js": [
                "crash.js"
            ]
        }
    ]
}